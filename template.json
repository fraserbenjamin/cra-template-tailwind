{
    "devDependencies": {
        "autoprefixer": "^9.8.4",
        "babel-plugin-macros": "^2.8.0",
        "postcss-cli": "^7.1.1",
        "tailwindcss": "^1.4.6",
        "twin.macro": "^1.5.0"
      },
    "dependencies": {
      "@testing-library/jest-dom": "^4.2.4",
      "@testing-library/react": "^9.5.0",
      "@testing-library/user-event": "^7.2.1",
      "react": "^16.13.1",
      "react-dom": "^16.13.1",
      "react-scripts": "3.4.1",
      "styled-components": "^5.1.1"
    },
    "scripts": {
      "start": "npm run watch:css & react-scripts start",
      "build": "npm run build:css react-scripts build",
      "test": "react-scripts test",
      "eject": "react-scripts eject",
      "build:css": "postcss src/assets/tailwind.css -o src/assets/styles.css",
      "watch:css": "postcss src/assets/tailwind.css -o src/assets/styles.css",
      "addDevDependencies": "npm i autoprefixer babel-plugin-macros postcss-cli tailwindcss twin.macro --save-dev"
    }
}
  